<h1>Wish List</h1>

<h3>Save your wishes here</h3>

<ul>
  {{#each wishes}}
  	<li>
  	   <p>ID: {{this.id}}</p>
       <p>Event: {{this.wish}}</p>
  	</li>
  {{/each}}
</ul>

<form action="/create" method="POST">
	<textarea type="text" name="event"></textarea>

	<button type="submit">Submit</button>
</form>
